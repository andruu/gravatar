{
    "name": "floatingpoint/gravatar",
    "description": "Gravatar library with Laravel 5 support.",
    "keywords" : ["laravel" , "laravel 5" , "gravatar" ] ,
    "homepage" : "https://github.com/floatingpoint/gravatar" ,
    "license" : "MIT",
    "authors": [
        {
            "name": "Kirk Bushell",
            "role": "Lead developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*",
        "thomaswelton/gravatarlib": "0.1.*"
    },
    "require-dev": {
      "mockery/mockery": "~0.9.3",
      "phpunit/phpunit": "~4.2."
    },
    "autoload": {
        "psr-4": {
            "FloatingPoint\\Gravatar\\": "src/",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
